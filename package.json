{
  "name": "tldr",
  "version": "1.0.1",
  "description": "Simplified and community-driven man pages",
  "author": "Romain Prieto",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rprieto/tldr.git"
  },
  "keywords": [
    "man",
    "unix",
    "commands"
  ],
  "main": "./bin/tldr",
  "bin": {
    "tldr": "./bin/tldr"
  },
  "preferGlobal": true,
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "NODE_ENV=development node server.js",
    "example": "NODE_ENV=development node ./bin/tldr tar",
    "test": "mocha test --recursive --reporter=spec",
    "watch": "mocha --watch test --recursive --reporter=min --growl"
  },
  "os": [
    "!win32"
  ],
  "dependencies": {
    "async": "~0.9.0",
    "colors": "~1.0.1",
    "commander": "~2.9.0",
    "cpr": "~0.3.2",
    "deep-extend": "~0.2.8",
    "lodash": "~2.4.1",
    "marked": "~0.3.1",
    "mkdirp": "~0.5.0",
    "ms": "~0.6.2",
    "request": "~2.44.0",
    "rimraf": "~2.2.6",
    "unhtml": "~0.1.0",
    "unzip2": "^0.2.5",
    "wrench": "~1.5.8"
  },
  "devDependencies": {
    "mocha": "~1.21.4",
    "should": "~4.0.4",
    "sinon": "~1.10.3"
  }
}
